{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "max-len": ["error", {"code": 100}],
        "new-cap": ["error", {"capIsNew": false}]
    }
}
